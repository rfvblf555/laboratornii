<?xml version="1.0" encoding="UTF-8" ?>
<Shell 
    x:Class="MVVMP10LAB.AppShell"
         xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
         xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
         xmlns:local="clr-namespace:MVVMP10LAB"
         Shell.FlyoutBehavior="Disabled"
         xmlns:views="clr-namespace:MVVMP10LAB.Views">
    <TabBar Route="TabBAr1">
        <Tab Title="Авторизация" >
            <ShellContent Title="Авторизация" Route="AuthTab"
                ContentTemplate="{DataTemplate views:LoginPage}"/>
            <ShellContent Title="Регистрация"
                ContentTemplate="{DataTemplate views:RegPage}"/>
        </Tab>

    </TabBar>
</Shell>

